{
  "name": "templatiq-sites",
  "version": "3.5.2",
  "main": "Gruntfile.js",
  "author": "Brainstorm Force",
  "description": "Starter Templates is all in one solution for complete starter sites, single page templates, blocks & images. This plugin offers the premium library of ready templates & provides quick access to beautiful Pixabay images that can be imported in your website easily.",
  "scripts": {
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "format:js": "wp-scripts format-js",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js ./inc/lib/onboarding/assets/src ./inc/lib/onboarding/packages/starter-templates-components/src",
    "lint:js:fix": "wp-scripts lint-js ./inc/lib/onboarding/assets/src ./inc/lib/onboarding/packages/starter-templates-components/src --fix",
    "lint:md:docs": "wp-scripts lint-md-docs",
    "lint:md:js": "wp-scripts lint-md-js",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "test:unit": "wp-scripts test-unit-js",
    "package": "rm -rf templatiq-sites && npm run build-package && grunt release-depracated",
    "release": "rm -rf templatiq-sites && npm run build-package && grunt release",
    "build": "wp-scripts build --entry=./inc/assets/js/src/ --output-path=./inc/assets/js/dist",
    "start": "wp-scripts start --entry=./inc/assets/js/src/ --output-path=./inc/assets/js/dist",
    "start:onboarding": "wp-scripts start --entry=./inc/lib/onboarding/assets/src --output-path=./inc/lib/onboarding/assets/dist",
    "build:onboarding": "wp-scripts build --entry=./inc/lib/onboarding/assets/src --output-path=./inc/lib/onboarding/assets/dist",
    "build-package": "npm run build && npm run build:onboarding && grunt rtl"
  },
  "keywords": [
    "reactjs",
    "components"
  ],
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.9.0 <7"
  },
  "license": "ISC",
  "devDependencies": {
    "@wordpress/e2e-test-utils": "^5.4.10",
    "@wordpress/env": "^8.9.0",
    "@wordpress/eslint-plugin": "^14.11.0",
    "@wordpress/jest-puppeteer-axe": "^6.9.0",
    "@wordpress/scripts": "^24.6.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-jest": "^27.2.3",
    "grunt": "^1.4.1",
    "grunt-bumpup": "^0.6.3",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-rtlcss": "^2.0.2",
    "grunt-text-replace": "^0.4.0",
    "grunt-wp-i18n": "^1.0.3",
    "jest-silent-reporter": "^0.5.0",
    "prettier": "npm:wp-prettier@^2.2.1-beta-1",
    "rtlcss-webpack-plugin": "^4.0.6",
    "grunt-wp-readme-to-markdown": "^2.0.1",
    "webpack": "^5.88.2"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^6.25.0",
    "@wordpress/components": "^22.1.0",
    "@wordpress/hooks": "^3.43.0",
    "@wordpress/html-entities": "^3.38.0",
    "@wordpress/i18n": "^4.41.0",
    "@wordpress/media-utils": "^4.29.0",
    "@wordpress/url": "^3.42.0",
    "canvas-confetti": "^1.9.0",
		"qs": "^6.10.2",
    "react-lottie-player": "^1.5.4",
    "react-router-dom": "^6.17.0",
    "styled-components": "^6.1.0"
  }
}